<?xml version="1.0" encoding="utf-8"?>

<Page
    x:Class="DQXLauncher.Windows.Views.LoginPages.LoginCompletedPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Loaded="LoginCompletedPage_OnLoaded"
    mc:Ignorable="d">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10" Margin="10">
        <Grid>
            <FontIcon FontSize="32" Glyph="&#xf13e;" Foreground="{StaticResource SuccessColorBrush}" />
            <FontIcon FontSize="32" Glyph="&#xf138;" Foreground="{StaticResource SuccessColorBrush}" />
        </Grid>
        <TextBlock>Logged in!</TextBlock>
    </StackPanel>
</Page>